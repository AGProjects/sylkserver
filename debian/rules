#!/usr/bin/make -f

#export DH_VERBOSE=1

%:
	dh  $@ --with python2

override_dh_clean:
	dh_clean
	rm -rf build dist MANIFEST

override_dh_install:
	install -m 0644 *.ini.sample debian/sylkserver/etc/sylkserver/
	dh_install

override_dh_installinit:
	dh_installinit --no-start

.PHONY: override_dh_clean override_dh_install override_dh_installinit
